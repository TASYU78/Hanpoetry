@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

@prefix hp:   <http://example.org/HanpoetryOWL#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix bf:   <http://id.loc.gov/ontologies/bibframe/> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

##################################################################
# Ontology metadata
##################################################################

<http://example.org/HanpoetryOWL>
    a owl:Ontology ;
    rdfs:label "Han Poetry Ontology for East Asian Literature"@en ;
    rdfs:label "東アジア漢詩のためのオントロジー"@ja ;
    rdfs:label "东亚汉诗本体"@zh ;
    dct:creator "HUANG YANJIE" ;
    dct:created "2025-04-11"^^xsd:date ;
    owl:versionInfo "v0.1" ;
    rdfs:comment "An ontology for representing Han poetry across East Asian traditions, using BIBFRAME, FOAF, and SKOS vocabularies for integration and semantic linkage."@en ;
    rdfs:comment "使用 BIBFRAME、FOAF 与 SKOS 词表构建的东亚汉诗表示本体，支持跨传统语义链接与集成。"@zh ;
    rdfs:comment "BIBFRAME・FOAF・SKOS 語彙を用いた、東アジア漢詩を表現するための本体である。"@ja .

##################################################################
# Core Classes
##################################################################

hp:Knowledge a owl:Class ;
    rdfs:label "Knowledge"@en ;
    rdfs:label "知识"@zh ;
    rdfs:label "知識"@ja ;
    rdfs:comment "Abstract conceptual knowledge related to Han poetry, including imagery, themes, and genres."@en ;
    rdfs:comment "与汉诗相关的抽象概念知识，例如意象、主题和体裁等。"@zh ;
    rdfs:comment "漢詩に関わる抽象的な概念知識。意象、テーマ、ジャンルなどを含む。"@ja .

hp:Poem a owl:Class ;
    rdfs:subClassOf bf:Work ;
    rdfs:label "Poem"@en ;
    rdfs:label "诗歌"@zh ;
    rdfs:label "詩"@ja ;
    rdfs:comment "A specific poetic work in Han poetry tradition, modeled as a subclass of bf:Work."@en ;
    rdfs:comment "东亚汉诗传统中的具体诗作，是 bf:Work 的子类。"@zh ;
    rdfs:comment "東アジア漢詩の個別作品であり、bf:Workのサブクラスとしてモデル化される。"@ja .

hp:Collection a owl:Class ;
    rdfs:subClassOf bf:Work ;
    rdfs:label "Collection"@en ;
    rdfs:label "作品集"@zh ;
    rdfs:label "詩集"@ja ;
    rdfs:comment "A poetic collection or anthology that contains multiple Poem instances."@en ;
    rdfs:comment "包含多首诗歌作品的诗集或文集。"@zh ;
    rdfs:comment "複数の詩を含む詩集・アンソロジー。"@ja .

foaf:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:label "人物"@zh ;
    rdfs:label "人物"@ja ;
    rdfs:comment "A person involved in Han poetry, such as poets, editors, or commentators."@en ;
    rdfs:comment "参与汉诗创作与传播的个人，例如诗人、注释者或编者。"@zh ;
    rdfs:comment "漢詩に関わる人物（詩人、編集者、注釈者など）を指す。"@ja .

hp:Manifestation a owl:Class ;
    rdfs:label "Manifestation"@en ;
    rdfs:label "表现形式"@zh ;
    rdfs:label "表現形態"@ja ;
    rdfs:comment "An intermediate entity that represents how a poem appears within a specific collection or edition."@en ;
    rdfs:comment "表示诗歌在特定作品集或版本中出现方式的中间实体。"@zh ;
    rdfs:comment "詩が特定の詩集・版においてどのように表現されているかを示す中間エンティティ。"@ja .

hp:Range a owl:Class ;
    rdfs:label "Range"@en ;
    rdfs:label "结构单元"@zh ;
    rdfs:label "構造範囲"@ja ;
    rdfs:comment "Structural subdivision of a manifestation, such as a section, chapter, or poem group."@en ;
    rdfs:comment "Manifestation 内的结构划分单元，如章节、节选、诗组等。"@zh ;
    rdfs:comment "表現形態の中での構造単位（章・詩群など）を示す。"@ja .

hp:Canvas a owl:Class ;
    rdfs:label "Canvas"@en ;
    rdfs:label "展示片段"@zh ;
    rdfs:label "表示キャンバス"@ja ;
    rdfs:comment "Atomic display unit that presents content such as a poem, stanza, or page."@en ;
    rdfs:comment "展示内容的基本单位，可为一首诗、一节诗句或一页内容。"@zh ;
    rdfs:comment "詩、詩節、ページなどを表示する最小単位の表示エリア。"@ja .

##################################################################
# Knowledge Subclasses for Inference
##################################################################


##################################################################
# Knowledge 子类体系扩展：Meaning（义）层
##################################################################

hp:Meaning a owl:Class ;
    rdfs:subClassOf hp:Knowledge ;
    rdfs:label "Meaning"@en ;
    rdfs:label "义"@zh ;
    rdfs:label "意味"@ja ;
    rdfs:comment "Thematic and conceptual content of Han poetry, including time, space, person, activity, imagery, emotion, and theme."@en ;
    rdfs:comment "汉诗的内容层面，包括时间、空间、人物、活动、意象、情感和主题等。"@zh ;
    rdfs:comment "漢詩における主題的・概念的な内容。時間、空間、人、活動、イメージ、感情、テーマなどを含む。"@ja .

# Time
hp:TimeConcept a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "Time Concept"@en ;
    rdfs:label "时间概念"@zh ;
    rdfs:label "時間の概念"@ja ;
    rdfs:comment "Temporal expressions in poetry, including dynasty, date, season, solar terms, and time of day."@en ;
    rdfs:comment "诗中涉及的时间表达，如朝代、年号、节气、时辰等。"@zh ;
    rdfs:comment "漢詩に見られる時間表現（朝代、季節、節気、時刻など）。"@ja .

# Space
hp:SpaceConcept a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "Space Concept"@en ;
    rdfs:label "空间概念"@zh ;
    rdfs:label "空間の概念"@ja ;
    rdfs:comment "Spatial expressions in poetry, including locations, landscapes, and named places."@en ;
    rdfs:comment "诗中涉及的空间场所，如地名、山川、城市、村庄等。"@zh ;
    rdfs:comment "漢詩における場所や空間表現（地名、山河、都市、農村など）。"@ja .

# Person
hp:PersonConcept a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "Person Concept"@en ;
    rdfs:label "人物概念"@zh ;
    rdfs:label "人物の概念"@ja ;
    rdfs:comment "Human-related concepts in poetry, including gender, appearance, profession, social role, and historical figures."@en ;
    rdfs:comment "诗中与人物相关的知识，如性别、容貌、职业、身份和历史人物等。"@zh ;
    rdfs:comment "人物に関する詩的表現（性別、容貌、職業、社会的役割、歴史的人物など）。"@ja .

hp:SceneConcept a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "Scene Concept"@en ;
    rdfs:label "景物概念"@zh ;
    rdfs:label "景物の概念"@ja ;
    rdfs:comment "Environmental and visual elements described in poetry, divided into natural and human-related scenery."@en ;
    rdfs:comment "诗歌中描绘的环境与景物，包括自然景物与人文景物两类。"@zh ;
    rdfs:comment "漢詩における環境や景観の描写。自然と人文の景物に分類される。"@ja .

##################################################################
# OWL 类定义：颜色概念 ColorConcept（继承 Meaning）
##################################################################

hp:ColorConcept a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "颜色概念"@zh ;
    rdfs:label "Color Concept"@en ;
    rdfs:label "色の概念"@ja ;
    rdfs:comment "诗歌中作为意象或象征出现的颜色类抽象概念。"@zh ;
    rdfs:comment "A conceptual class representing colors as symbolic or descriptive imagery in poetry."@en ;
    rdfs:comment "詩において象徴的・描写的な意象として用いられる色の概念。"@ja .

# Activity
hp:Activity a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "Activity"@en ;
    rdfs:label "活动"@zh ;
    rdfs:label "活動"@ja ;
    rdfs:comment "Activities in poetry, divided into individual and collective/social actions."@en ;
    rdfs:comment "诗中描绘的个体行为与社会集体行为。"@zh ;
    rdfs:comment "漢詩に見られる個人的および社会的な活動。"@ja .

hp:IndividualActivity a owl:Class ; rdfs:subClassOf hp:Activity ; rdfs:label "Individual Activity"@en ; rdfs:label "个体活动"@zh ; rdfs:label "個人活動"@ja .
hp:SocialActivity a owl:Class ; rdfs:subClassOf hp:Activity ; rdfs:label "Social Activity"@en ; rdfs:label "社会活动"@zh ; rdfs:label "社会活動"@ja .

#Allusion 
hp:Allusion a owl:Class ;
    rdfs:subClassOf hp:Meaning ;
    rdfs:label "Allusion"@en ;
    rdfs:label "典故"@zh ;
    rdfs:label "典拠"@ja ;
    rdfs:comment "A reference in poetry to a known historical, literary, or cultural source."@en ;
    rdfs:comment "诗歌中引用历史、文学或文化典籍的内容，称为典故。"@zh ;
    rdfs:comment "詩に登場する歴史・文学・文化典拠への言及。"@ja .

##################################################################
# Knowledge 子类体系扩展：Form（体）层
##################################################################

hp:Form a owl:Class ;
    rdfs:subClassOf hp:Knowledge ;
    rdfs:label "Form"@en ;
    rdfs:label "体"@zh ;
    rdfs:label "形式"@ja ;
    rdfs:comment "Formal structures of Han poetry, including poetic style, structure, syntax, and parallelism."@en ;
    rdfs:comment "汉诗的形式结构层面，包括诗体、章法结构、句式和对仗等。"@zh ;
    rdfs:comment "漢詩の形式的な構造。詩体、構成、句法、対句などを含む。"@ja .

hp:Genre a owl:Class ;
    rdfs:subClassOf hp:Form ;
    rdfs:label "Genre"@en ;
    rdfs:label "诗体"@zh ;
    rdfs:label "詩体"@ja ;
    rdfs:comment "Poetic genres such as ancient-style poetry, regulated verse, or quatrain."@en ;
    rdfs:comment "如古体诗、近体诗、律诗、绝句等各种诗体。"@zh ;
    rdfs:comment "古体詩、近体詩、律詩、絶句などの詩のジャンル。"@ja .

hp:Structure a owl:Class ;
    rdfs:subClassOf hp:Form ;
    rdfs:label "Structure"@en ;
    rdfs:label "结构章法"@zh ;
    rdfs:label "構成・章法"@ja ;
    rdfs:comment "Overall structural organization of a poem, such as number of lines, stanza arrangement, and compositional patterns."@en ;
    rdfs:comment "诗的总体结构安排，如句数、章法、对称结构等。"@zh ;
    rdfs:comment "詩の構成構造。句数、段落構成、章法などを含む。"@ja .

hp:Syntax a owl:Class ;
    rdfs:subClassOf hp:Form ;
    rdfs:label "Syntax"@en ;
    rdfs:label "句法结构"@zh ;
    rdfs:label "句法構造"@ja ;
    rdfs:comment "Syntactic features of poetic lines, including inversion, ellipsis, and sentence types."@en ;
    rdfs:comment "诗句中的语法特征，如倒装、省略、句型变化等。"@zh ;
    rdfs:comment "詩の句に見られる文法的特徴（倒置、省略、文型など）。"@ja .

hp:Parallelism a owl:Class ;
    rdfs:subClassOf hp:Form ;
    rdfs:label "Parallelism"@en ;
    rdfs:label "对仗结构"@zh ;
    rdfs:label "対句構造"@ja ;
    rdfs:comment "Parallel alignment of phrases or lines in poetry, often involving syntactic, semantic, and rhythmic symmetry."@en ;
    rdfs:comment "诗句之间的对称性安排，可能涉及词性、意义与音节的对仗。"@zh ;
    rdfs:comment "詩句間の対称的な構造（文法、意味、音の一致など）。"@ja .

##################################################################
# Knowledge 子类体系扩展：Sound（音）层
##################################################################

hp:Sound a owl:Class ;
    rdfs:subClassOf hp:Knowledge ;
    rdfs:label "Sound"@en ;
    rdfs:label "音"@zh ;
    rdfs:label "音韻"@ja ;
    rdfs:comment "Phonological and prosodic features of Han poetry, including tonal patterns and rhyme schemes."@en ;
    rdfs:comment "汉诗中的音韵特征，包括平仄、押韵等格律要素。"@zh ;
    rdfs:comment "漢詩における音韻的特徴。平仄や韻脚などの詩の格律に関する要素。"@ja .

hp:Tone a owl:Class ;
    rdfs:subClassOf hp:Sound ;
    rdfs:label "Tone"@en ;
    rdfs:label "平仄"@zh ;
    rdfs:label "声調（平仄）"@ja ;
    rdfs:comment "Tonality in classical Chinese poetry, especially the use of level and oblique tones for prosodic balance."@en ;
    rdfs:comment "古典汉诗中的声调系统，特别是平声与仄声的使用规律。"@zh ;
    rdfs:comment "漢詩における声調。平声と仄声の配置による韻律的な調和。"@ja .

hp:Rhyme a owl:Class ;
    rdfs:subClassOf hp:Sound ;
    rdfs:label "Rhyme"@en ;
    rdfs:label "押韵"@zh ;
    rdfs:label "韻脚"@ja ;
    rdfs:comment "Rhyming schemes used in poetry, such as end rhyme and tonal rhyme in regulated verse."@en ;
    rdfs:comment "诗歌中使用的押韵规则，包括尾韵、同声母韵等。"@zh ;
    rdfs:comment "詩に用いられる韻脚（句末の押韻）などの韻律的な配置。"@ja .

##################################################################
# Properties
##################################################################

hp:hasSKOSConcept a owl:ObjectProperty ;
    rdfs:label "has SKOS Concept"@en ;
    rdfs:label "关联SKOS概念"@zh ;
    rdfs:label "SKOS概念との関連"@ja ;
    rdfs:domain hp:Knowledge ;
    rdfs:range skos:Concept ;
    rdfs:comment "Links a knowledge concept to a corresponding SKOS concept for multilingual or hierarchical vocabulary alignment."@en ;
    rdfs:comment "将知识类概念连接到相应的SKOS词汇概念，用于多语言或层级语义对齐。"@zh ;
    rdfs:comment "SKOS語彙の概念と接続し、多言語対応や階層的語彙の整合に用いる。"@ja .

hp:denotesEntity a owl:ObjectProperty ;
    rdfs:label "denotes Entity"@en ;
    rdfs:label "指称实体"@zh ;
    rdfs:label "実体とのリンク"@ja ;
    rdfs:domain hp:Knowledge ;
    rdfs:range owl:Thing ;  # 可连接任何语义实体
    rdfs:comment "Links a poetic concept to the real-world or semantically defined entity it denotes or symbolizes."@en ;
    rdfs:comment "将诗歌中的概念与其所象征或指代的现实世界实体建立连接。"@zh ;
    rdfs:comment "詩的概念と、それが象徴する現実世界の実体を結びつける。"@ja .


##################################################################
# Poem 类 Properties 属性定义
##################################################################

#hasPoeticConcept
hp:hasPoeticConcept a owl:ObjectProperty ;
    rdfs:domain hp:Poem ;
    rdfs:range skos:Concept ;
    rdfs:label "has poetic concept"@en ;
    rdfs:label "关联诗歌概念"@zh ;
    rdfs:label "詩の概念との関連"@ja ;
    rdfs:comment "Links a poem to a SKOS concept representing style, type, or symbolic content."@en ;
    rdfs:comment "诗本身的 SKOS 概念连接。"@zh ;
    rdfs:comment "詩のSKOS 概念との関連。"@ja .

#hasTitle
bf:title a owl:DatatypeProperty ;
    rdfs:label "title"@en ;
    rdfs:label "标题"@zh ;
    rdfs:label "題名"@ja ;
    rdfs:comment "The title of the poem or collection, typically as it appears in a source text."@en ;
    rdfs:comment "诗歌或诗集的标题，通常来自原始文本中的题名。"@zh ;
    rdfs:comment "詩や詩集の題名。原典に記載された名称に対応する。"@ja .


# hasAuthor
hp:hasAuthor a owl:ObjectProperty ;
    rdfs:label "has author"@en ;
    rdfs:label "拥有作者"@zh ;
    rdfs:label "作者とのリンク"@ja ;
    rdfs:domain hp:Poem ;
    rdfs:range foaf:Person ;
    rdfs:comment "Connects a poem to its author, modeled as a foaf:Person."@en ;
    rdfs:comment "将诗歌与其创作者连接，创作者使用 FOAF 的 Person 表示。"@zh ;
    rdfs:comment "詩とその作者（foaf:Person）を結びつける。"@ja .

# hasCreationTime
hp:hasCreationTime a owl:DatatypeProperty ;
    rdfs:label "has creation time"@en ;
    rdfs:label "创作时间"@zh ;
    rdfs:label "作成時期"@ja ;
    rdfs:domain hp:Poem ;
    rdfs:range xsd:date ;
    rdfs:comment "The date when the poem was composed or first recorded."@en ;
    rdfs:comment "诗歌的创作或首次记录时间。"@zh ;
    rdfs:comment "詩の創作または記録された日付。"@ja .

# hasExpression
hp:hasExpression a owl:ObjectProperty ;
    rdfs:label "has expression"@en ;
    rdfs:label "表达方式"@zh ;
    rdfs:label "表現とのリンク"@ja ;
    rdfs:domain hp:Poem ;
    rdfs:range skos:Concept ;
    rdfs:comment "Links a poem to a SKOS concept describing its expressive style (e.g., historical, object-descriptive)."@en ;
    rdfs:comment "将诗歌与其表达风格（如咏史、咏物等）连接到 SKOS 概念。"@zh ;
    rdfs:comment "詩の表現スタイル（詠史、詠物など）を表すSKOS概念との接続。"@ja .

# hasKnowledge
hp:hasKnowledge a owl:ObjectProperty ;
    rdfs:label "has knowledge"@en ;
    rdfs:label "关联知识"@zh ;
    rdfs:label "関連知識"@ja ;
    rdfs:domain hp:Poem ;
    rdfs:range hp:Knowledge ;
    rdfs:comment "Links a poem to relevant knowledge items such as imagery, theme, or emotion."@en ;
    rdfs:comment "将诗歌与相关知识项（如意象、主题、情感等）建立联系。"@zh ;
    rdfs:comment "詩と意象・主題・感情などの知識項目を接続する。"@ja .

# hasCanvas
hp:isRepresentedIn a owl:ObjectProperty ;
    owl:inverseOf hp:representsPoem ;
    rdfs:domain hp:Poem ;
    rdfs:range hp:Canvas ;
    rdfs:label "is represented in"@en ;
    rdfs:label "被展示于"@zh ;
    rdfs:label "に表されている"@ja ;
    rdfs:comment "Inverse of representsPoem, used to trace from a poem to its instantiations in canvases."@en ;
    rdfs:comment "表示某首诗在具体 Canvas 中的展示情况。"@zh ;
    rdfs:comment "ある詩がどの Canvas に現れるかを示す逆プロパティ。"@ja .


# hasText
hp:hasText a owl:DatatypeProperty ;
    rdfs:label "has text"@en ;
    rdfs:label "原文内容"@zh ;
    rdfs:label "原文テクスト"@ja ;
    rdfs:domain hp:Poem ;
    rdfs:range rdf:langString ;
    rdfs:comment "The original textual content of the poem."@en ;
    rdfs:comment "诗歌的原始文本内容。"@zh ;
    rdfs:comment "詩の原文内容。"@ja .


##################################################################
# Collection 类 Properties 属性定义
##################################################################

hp:Collection a owl:Class ;
    rdfs:subClassOf bf:Work ;
    rdfs:label "Collection"@en ;
    rdfs:label "诗集"@zh ;
    rdfs:label "詩集"@ja ;
    rdfs:comment "A poetry collection that contains multiple manifestations of poetic content."@en ;
    rdfs:comment "包含多种诗歌表现形式的诗集。"@zh ;
    rdfs:comment "さまざまな詩の表現形態を含む詩集。"@ja .

hp:hasCollectionConcept a owl:ObjectProperty ;
    rdfs:domain hp:Collection ;
    rdfs:range skos:Concept ;
    rdfs:label "has collection concept"@en ;
    rdfs:label "关联诗集概念"@zh ;
    rdfs:label "詩集概念との関連"@ja ;
    rdfs:comment "Links the collection to a SKOS concept (e.g., period, genre, function)."@en .

hp:hasDescription a owl:DatatypeProperty ;
    rdfs:domain hp:Collection ;
    rdfs:range rdf:langString ;
    rdfs:label "has description"@en ;
    rdfs:label "简介"@zh ;
    rdfs:label "説明"@ja ;
    rdfs:comment "Short summary or note about the collection."@en .

hp:hasCreationTime a owl:DatatypeProperty ;
    rdfs:domain hp:Collection ;
    rdfs:range xsd:date ;
    rdfs:label "has creation time"@en ;
    rdfs:label "创作时间"@zh ;
    rdfs:label "編纂時期"@ja ;
    rdfs:comment "Compilation or publication date of the collection."@en .

hp:hasAuthor a owl:ObjectProperty ;
    rdfs:domain hp:Collection ;
    rdfs:range foaf:Person ;
    rdfs:label "has author"@en ;
    rdfs:label "编者/作者"@zh ;
    rdfs:label "編者・作者"@ja ;
    rdfs:comment "Compiler or editor of the collection."@en .

hp:hasManifestation a owl:ObjectProperty ;
    rdfs:domain hp:Collection ;
    rdfs:range hp:Manifestation ;
    rdfs:label "has manifestation"@en ;
    rdfs:label "包含表现形式"@zh ;
    rdfs:label "表現形態を含む"@ja ;
    rdfs:comment "Links to all Manifestation entities under this Collection."@en .

#################################################################
#Manifestation Properties 属性定义
#################################################################
hp:Manifestation a owl:Class ;
    rdfs:label "Manifestation"@en ;
    rdfs:label "表现形式"@zh ;
    rdfs:label "表現形態"@ja ;
