    a hp:SceneConcept ;
    rdfs:label "蜀道"@zh , "Shu Road"@en , "蜀の道"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/SceneConcept/NaturalScene/GeographicalFeature/ShuRoad> ;
