##################################################################
# 知识实例：抒情（主题）
##################################################################

<http://example.org/hanpoetry/resource/knowledge/ThemeSentimentLyric> a hp:ThemeConcept ;
    rdfs:label "抒情"@zh ;
    rdfs:label "Sentiment / Lyric"@en ;
    rdfs:label "抒情"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/ThemeConcept/SentimentLyric> ;
    rdfs:comment "以抒发个人情感为主，不拘内容题材。"@zh ;
    rdfs:comment "A general theme focused on the expression of personal feelings or emotions in poetry."@en ;
    rdfs:comment "個人的感情や情緒を自由に表現する詩の主題。"@ja .

<http://example.org/hanpoetry/resource/knowledge/ThemeFarewell> a hp:ThemeConcept ;
    rdfs:label "送别"@zh ;
    rdfs:label "Farewell"@en ;
    rdfs:label "別れ"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/ThemeConcept/Farewell> ;
    rdfs:comment "表达亲人、朋友、恋人等在离别场景中的情感抒发，是中国古典诗歌常见主题。"@zh ;
    rdfs:comment "A poetic theme expressing the emotions associated with parting between friends, lovers, or kin."@en ;
    rdfs:comment "友人や親しい人との別れを詠んだ詩の主題。"@ja .

<http://example.org/hanpoetry/resource/knowledge/ThemeHomesickness> a hp:ThemeConcept ;
    rdfs:label "思乡"@zh ;
    rdfs:label "Homesickness"@en ;
    rdfs:label "望郷"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/ThemeConcept/Homesickness> ;
    rdfs:comment "表达因身处异乡、漂泊离散而引发的对故土亲人的思念之情。"@zh ;
    rdfs:comment "A theme expressing longing for one's homeland and family, common in exile or travel poetry."@en ;
    rdfs:comment "異郷や旅先での故郷や親族への思いを詠う詩の主題。"@ja .

<http://example.org/hanpoetry/resource/knowledge/ThemeNostalgiaAntiquity> a hp:ThemeConcept ;
    rdfs:label "怀古"@zh ;
    rdfs:label "Nostalgia / Antiquity"@en ;
    rdfs:label "懐古"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/ThemeConcept/NostalgiaAntiquity> ;
    rdfs:comment "借古人古事寄托今情，抒发盛衰兴亡之感。"@zh ;
    rdfs:comment "A theme that evokes reflection on historical events or figures, often conveying melancholy or moral insight."@en ;
    rdfs:comment "歴史や過去の人物を通して、感慨や感情を表す詩の主題。"@ja .

<http://example.org/hanpoetry/resource/knowledge/ThemeBorderCampaign> a hp:ThemeConcept ;
    rdfs:label "边塞征战"@zh ;
    rdfs:label "Border Campaign"@en ;
    rdfs:label "辺塞の戦い"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/ThemeConcept/MilitaryBorderCampaign> ;
    rdfs:comment "描写征战边塞、戍边守土的诗歌主题。"@zh ;
    rdfs:comment "A poetic theme depicting military expeditions and garrison life on the frontier."@en ;
    rdfs:comment "辺境の戦いや防衛を詠み、戦争を表す主題。"@ja .

<http://example.org/hanpoetry/resource/knowledge/ThemeLandscapeNature> a hp:ThemeConcept ;
    rdfs:label "自然山水"@zh ;
    rdfs:label "Landscape / Nature"@en ;
    rdfs:label "自然山水"@ja ;
    hp:hasSKOSConcept <http://example.org/HanpoetryConcept#KnowledgeConcept/MeaningConcept/ThemeConcept/LandscapeNature> ;
    rdfs:comment "描写自然景物如山川、江河、风月等景象，抒发诗人对大自然的赞美、感慨与寄情。"@zh ;
    rdfs:comment "A poetic theme describing natural landscapes such as mountains, rivers, and celestial phenomena, often conveying admiration, reflection, or personal sentiments."@en ;
    rdfs:comment "山や川、風や月などの自然の風景を詠み、自然への賛美や感慨、思いを込めた詩の主題。"@ja .


# 蜀道意象
